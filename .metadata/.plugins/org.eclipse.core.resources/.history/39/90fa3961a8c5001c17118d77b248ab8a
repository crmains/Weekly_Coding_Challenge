package robot_in_a_grid;

import java.util.Scanner;

public class Robot_in_a_grid {

	public static void main(String[] args) {
		int[] robot = new int[2];
		robot[0] = 0; robot [1] = 0;
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("What is the number of rows in the grid.\n>");
		int rows = scan.nextInt();
		
		System.out.println("What is the number of columns in the grid.\n>");
		int colmns = scan.nextInt();
		
		int[][] grid = new int[rows][colmns];
		
		for(int i = 0; i < rows; i++) {
			for(int j = 0; j < colmns; j++) {
				System.out.print(grid[i][j]);
			}
			System.out.print("\n");
		}

	}

}
